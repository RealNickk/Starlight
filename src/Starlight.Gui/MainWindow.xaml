<Window x:Class="Starlight.Gui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Starlight.Gui"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="400">
    <Grid>
        <GroupBox Header="Stream Snipe" Margin="10,10,44,41">
            <Grid Height="173" Margin="0,0,-2,0">
                <Label Content="User ID" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                <TextBox x:Name="UidBox" HorizontalAlignment="Left" Margin="64,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164"/>
                <Label Content="Place ID" HorizontalAlignment="Left" Margin="6,33,0,0" VerticalAlignment="Top"/>
                <TextBox x:Name="PlidBox" HorizontalAlignment="Left" Margin="64,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164"/>
                <Button Content="Join Game" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top" Width="218" Click="Button_Click"/>
            </Grid>
        </GroupBox>

    </Grid>
</Window>
